{
    "apiVersion": "kubedirector.hpe.com/v1beta1",
    "kind": "KubeDirectorApp",
    "metadata": {
      "name": "kd-webapp"
    },
    "spec": {
      "label": {
        "name": "statefull-webapp",
        "description": "StefDS sample KubeDirector App"
      },
      "distroID": "kd-webapp",
      "version": "1.0",
      "configSchemaVersion": 8,
      "config": {
        "roleServices": [
          {
            "roleID": "webappRole",
            "serviceIDs": [
              "webappui"
            ]
          }
        ],
        "selectedRoles": [
          "webappRole"
        ]
      },
      "services": [
        {
          "id": "webappui",
          "label": {
            "name": "WebappUI"
          },
          "endpoint": {
            "port": 8090,
            "isDashboard": true,
            "hasAuthToken": false,
            "urlScheme": "http"
          }
        }
      ],
      "roles": [
        {
          "id": "webappRole",
          "cardinality": "1+",
          "minResources": {
            "memory": "100Mi",
            "cpu": "1"
          }
        }
      ],
      "defaultImageRepoTag": "docker.io/stefds/webapp",
      "defaultPersistDirs": [
        "/usr/src/app/private"
      ],
      "capabilities": [],
      "systemdRequired": false,
      "logoURL": "https://raw.githubusercontent.com/bluedatainc/solutions/master/MLOps/logos/cassandra-3.11.png"
    }
  }